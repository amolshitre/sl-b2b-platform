<?xml version="1.0" encoding="UTF-8"?>
<beans
	xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

	<bean id="changePasswordAction" class="com.newco.marketplace.web.action.provider.ChangePasswordAction" scope="prototype" >
		<constructor-arg index="0" ref="changePasswordDelegates"/>
		<constructor-arg index="1" ref="changePasswordDto"/>
		<constructor-arg index="2" ref="forgetUserName"/>
		<constructor-arg index="3" ref="securityBean"/>																																																																																																																																																																																																			
		<constructor-arg index="4" ref="loginDelegates"/>
		<constructor-arg index="5" ref="SODashBoardDelegateImpl"/>
		<constructor-arg index="6" ref="userProfileDaoImpl"/>
	</bean>
	
</beans>
