<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="OrderFulfillment.Remote" thread-count="1">
    <test name="RecallCompletionTest" >
        <groups><run><include name="unit" /></run></groups>
        <classes>
            <class name="com.servicelive.orderfulfillment.remote.test.OrderFulfillmentWorkflowTests">
                <methods>
                    <include name="testNotificationTaskListener"/>
                    <include name="createServiceOrder"/>
                    <include name="postServiceOrder"/>
                    <include name="acceptServiceOrder"/>
                    <include name="activateOrder"/>
                    <include name="completeOrder"/>
                    <include name="recallOrderCompletion"/>
                    <include name="completeOrderAfterRecall"/>
                    <include name="closeAfterRecall"/>
                </methods>
            </class>
        </classes>
    </test>
</suite>

